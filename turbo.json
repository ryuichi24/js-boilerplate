{
  "$schema": "https://turbo.build/schema.json",
  "globalDependencies": [],
  "tasks": {
    "lint": {},
    "format": {},
    "@js-boilerplate/playground-node#dev": {
      "cache": false,
      "persistent": true,
      "dependsOn": []
    },
    "@js-boilerplate/playground-ts-node#dev": {
      "cache": false,
      "persistent": true,
      "dependsOn": []
    },
    "@js-boilerplate/playground-react#dev": {
      "cache": false,
      "persistent": true,
      "dependsOn": []
    },
    "@js-boilerplate/playground-ts-react#dev": {
      "cache": false,
      "persistent": true,
      "dependsOn": []
    },
    "build": {
      "dependsOn": ["^@js-boilerplate/decorator-factory#build"]
    },
    "@js-boilerplate/decorator-factory#build": {
      "dependsOn": ["^@js-boilerplate/util#build"]
    },
    "@js-boilerplate/util#build": {
      "dependsOn": ["^@js-boilerplate/type-util#build"]
    },
    "@js-boilerplate/type-util#build": {
      "dependsOn": []
    }
  }
}
